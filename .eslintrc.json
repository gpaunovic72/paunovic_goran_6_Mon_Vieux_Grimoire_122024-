{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["off"],

    "no-restricted-globals": ["off"],
    "no-console": ["off"],

    "no-use-before-define": ["off"],
    "operator-linebreak": ["off"],

    "comma-dangle": ["off"],

    "no-unused-vars": ["off"],
    "no-underscore-dangle": ["off"],

    "consistent-return": ["off"]
  }
}
